<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.swdc.skybox.ui.controller.ConfigController">
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </BorderPane.margin>
         <styleClass>
            <String fx:value="content" />
            <String fx:value="config-tab" />
         </styleClass>
      </TabPane>
   </center>
   <top>
      <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="600.0" styleClass="content" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#onConfigSave" prefHeight="30.0" prefWidth="59.0" styleClass="btn" text="保存">
               <HBox.margin>
                  <Insets right="8.0" top="4.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
